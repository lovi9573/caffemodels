LIBS += -lprotobuf

build: protobuf 
	c++ -rdynamic $(FLAGS) $(BLAHFLAGS) $(INCLUDES) $(LIBPATHS)  expose.cxx $(LOCALLIBS)  -o expose $(STATICLIBS) $(BLAH) $(LIBS)

	
